{
    "manifest_version": 3,
    "name": "Youtube sync",
    "version": "0.1",
    "default_locale": "vi",
    "description": "Một tiện ích mở rộng cho phép đồng bộ hóa video đã xem trên youtube",
    "icons": {},
    "background": {
        "service_worker": "/assets/js/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "/assets/js/content.js"
            ]
        }
    ]
}